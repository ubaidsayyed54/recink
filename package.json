{
  "name": "recink",
  "version": "1.12.7",
  "description": "Rethink CI for JavaScript applications",
  "keywords": [
    "REciNK",
    "Testing",
    "End2End",
    "Unit",
    "Development",
    "Tools",
    "Travis",
    "Coverage"
  ],
  "homepage": "https://github.com/MitocGroup/recink",
  "bugs": {
    "url": "https://github.com/MitocGroup/recink/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Mitoc Group",
    "email": "hello@mitocgroup.com",
    "url": "https://www.mitocgroup.com"
  },
  "contributors": [
    {
      "name": "AlexanderC",
      "email": "acucer@mitocgroup.com"
    }
  ],
  "bin": {
    "recink": "./bin/cli.js"
  },
  "main": "src/recink.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MitocGroup/recink.git"
  },
  "scripts": {
    "docs": "npm run docs:diagram && npm run docs:api",
    "docs:diagram": "node node_modules/.bin/dependo -f cjs src/ > docs/module-diagram.html",
    "docs:api": "rm -rf docs/api && node node_modules/.bin/esdoc",
    "postinstall": "node node-bin/ensure-peer-dependencies.js",
    "prepublish": "npm run snyk-protect; npm run transpile",
    "transpile": "node `npm bin`/babel ./src -d ./src",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "aws-sdk": "^2.49.0",
    "caporal": "^0.5.0",
    "chalk": "^1.1.3",
    "columnify": "^1.5.4",
    "debug": "^2.6.8",
    "dot-object": "^1.5.4",
    "fs-extra": "^3.0.1",
    "global-modules": "^1.0.0",
    "istanbul": "^0.4.5",
    "jag": "^1.0.14",
    "md5-file": "^3.1.1",
    "md5-hex": "^2.0.0",
    "merge": "^1.2.0",
    "mocha": "^3.3.0",
    "nightmare": "^2.10.0",
    "object.entries": "^1.0.4",
    "ora": "^1.2.0",
    "package-hash": "^2.0.0",
    "pify": "^3.0.0",
    "pretty-bytes": "^4.0.2",
    "print": "^1.0.2",
    "progress-stream": "^2.0.0",
    "puppeteer": "^0.10.0",
    "readdir-enhanced": "^1.5.2",
    "resolve-package": "^1.0.1",
    "sandboxed-module": "MitocGroup/node-sandboxed-module",
    "testcafe": "^13.1.1",
    "url-exists": "^1.0.3",
    "yaml-js": "^0.1.5",
    "yamljs": "^0.2.10",
    "snyk": "^1.41.1"
  },
  "peerDependencies": {
    "twig": "^1.10.5",
    "travis-lint": "^1.0.0",
    "travis-encrypt": "MitocGroup/node-travis-encrypt",
    "github-slug": "^2.0.0",
    "parse-github-repo-url": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "dependo": "^0.1.6",
    "esdoc": "^0.5.2",
    "esdoc-node": "^1.0.2"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "preferGlobal": true,
  "snyk": true
}
